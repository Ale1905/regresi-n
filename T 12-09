x1<-c(5,12,14,17,23,30,40,47,55,67,72,81,96,112,127)
y1<-c(4,10,13,15,15,25,27,46,38,46,53,70,82,99,100)
mod1<-lm(y1~x1)
summary(mod1)
anova(mod1)
qt(.025,13) #Estadístico t con a=.05
